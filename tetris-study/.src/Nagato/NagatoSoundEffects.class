' Gambas class file

Private $soundSucsess As Sound
Private $soundEnd As Sound
Private $soundKick As Sound

Public Sub _new()
    
    $soundSucsess = Sound.Load("success.wav")
    $soundEnd = Sound.Load("end-bell.wav")
    $soundKick = Sound.Load("kick.wav")
    
End

Public Sub Success()
        
    $soundSucsess.Play()
        
End

Public Sub EndBell()
        
    $soundEnd.Play()
        
End

Public Sub Kick()
        
    $soundKick.Play()
        
End
